<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

    <definition name="deliveryTableView" extends="default">
        <put-attribute name="body" value="/WEB-INF/views/delivery/deliveryTableView.jsp"/>

        <put-list-attribute name="jsNames">
            <add-attribute value="app/application.js"/>
            <add-attribute value="app/delivery/deliveryTable.js"/>
        </put-list-attribute>

        <put-list-attribute name="styleNames">
            <add-attribute value="app/application.css"/>
        </put-list-attribute>

    </definition>

    <definition name="HomeDelivery" extends="default">
        <put-attribute name="body" value="/WEB-INF/views/delivery/HomeDelivery.jsp"/>

        <put-list-attribute name="jsNames">
            <add-attribute value="app/application.js"/>
        </put-list-attribute>

        <put-list-attribute name="styleNames">
            <add-attribute value="app/application.css"/>
        </put-list-attribute>

    </definition>


    <definition name="addNewDelivery" extends="default">
        <put-attribute name="body" value="/WEB-INF/views/delivery/AddNewDelivery.jsp"/>

        <put-list-attribute name="jsNames">
            <add-attribute value="app/application.js"/>
        </put-list-attribute>

        <put-list-attribute name="styleNames">
            <add-attribute value="app/application.css"/>
        </put-list-attribute>

    </definition>

</tiles-definitions>
